package openjsip.proxy;

import java.io.*;
import java.net.*;

public class TCP_client extends Thread {
	
	private int port;
	private String dest;
	private byte [] addr;
	
	public TCP_client() {
		this.port = 12121;
		this.addr[0] = (byte) 193;
		this.addr[1] = (byte) 168;
		this.addr[2] = (byte) 1;
		this.addr[3] = (byte) 1;
		try {
			InetAddress dest = InetAddress.getByAddress(this.addr);
			
		} catch (UnknownHostException h) {
			h.printStackTrace();
		}
	}
}
