package openjsip.proxy;

import java.io.*;
import java.net.*;

public class TCP_client extends Thread {
	
	private int port;
	private String dest;
	private byte [] addr;
	
	public TCP_client() {
		this.port = 12121;
		this.addr[0] = (byte);
		try {
			InetAddress dest = InetAddress.getByAddress(null);
		} catch (UnknownHostException h) {
			h.printStackTrace();
		}
	}
}
